# Production environment variables (add to Vercel dashboard)
NEXTAUTH_SECRET=your-nextauth-secret-key-here
NEXTAUTH_URL=https://your-app.vercel.app

# Google OAuth (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Vercel Postgres (automatically provided by Vercel)
POSTGRES_URL=your-postgres-connection-string
POSTGRES_PRISMA_URL=your-postgres-prisma-connection-string
POSTGRES_URL_NO_SSL=your-postgres-no-ssl-connection-string
POSTGRES_URL_NON_POOLING=your-postgres-non-pooling-connection-string
POSTGRES_USER=your-postgres-user
POSTGRES_HOST=your-postgres-host
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DATABASE=your-postgres-database

# Vercel Blob (automatically provided when you add Vercel Blob)
BLOB_READ_WRITE_TOKEN=your-blob-token